{
  "name": "api-clients/rx",
  "description": "RxPHP utilities",
  "license": "MIT",
  "authors": [
    {
      "name": "Cees-Jan Kiewiet",
      "email": "ceesjank@gmail.com"
    }
  ],
  "require": {
    "php": "^8 || ^7.4",
    "react/event-loop": "^1.2",
    "react/promise": "^2.7",
    "reactivex/rxphp": "^2.0"
  },
  "require-dev": {
    "api-clients/test-utilities": "^3.1.0"
  },
  "autoload": {
    "psr-4": {
      "ApiClients\\Tools\\Rx\\": "src/"
    },
    "files": [
      "src/functions_include.php",
      "src/bootstrap.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "ApiClients\\Tests\\Tools\\Rx\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "7.4.7"
    }
  }
}
